@model List<GetAllDestinationQueryResult>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>CQRS TUR VERİLERİ</h1>

<br />

<table class="table table-bordered">

    <tr>
        <th>#</th>
        <th>Şehir</th>
        <th>Gün gece</th>
        <th>Fiyat</th>
        <th>Kapasite</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
   {
       <tr>
           <td>@item.id</td>
           <td>@item.city</td>
           <td>@item.daynight</td>
           <td>@item.price</td>
           <td>@item.capacity</td>
           <td><a href="#" class="btn btn-primary">Sil</a></td>
           <td><a href="#" class="btn btn-info">Güncelle</a></td>
       </tr>
   }
</table>
<a href="#" class="btn btn-outline-info">Yeni Rota Ekle</a>